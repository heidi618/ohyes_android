<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mywifi_demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_final"
        android:label="PriFit"
        android:roundIcon="@mipmap/icon_final_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.inputCertificate"></activity>
        <activity android:name=".Activity.inputSeonsorActivity" />
        <activity android:name=".Activity.physicInfoActivity" />
        <activity android:name=".Activity.ListActivity" />
        <activity android:name=".Activity.JoinActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.hardware.camera"
        android:required="true"/>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>